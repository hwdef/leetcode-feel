Solution
{
    .. XyLang\Library

    twoSum (nums:[]I32)->(v:[]I32)
    {
        hashmap := [I32]I32.{}
        nums.@ i -> v
        {
            temp := target - v
            ? hashmap.ContainsKey.(temp)
            {
                <- ( [i, hashmap.[temp]] )
            }
            ?
            {
                hashmap.[v] = i
            }
        }
        <- (null)
    }
}