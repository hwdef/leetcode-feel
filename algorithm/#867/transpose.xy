Solution
{
    .. XyLang\Library

    transpose (A:[][]I32)->(v:[][]I32)
    {
        l1 := A.Count
        l2 := A.[0].Count
        result := [][]I32.{}
        [0<<l2-1].@ i 
        {
            temp := []I32.{}
            [0<<l1-1].@ j
            {
                temp += A.[j].[i]
            }
            result += temp 
        }
        <- (result)
    }
}