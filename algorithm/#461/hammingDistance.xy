Solution
{
    .. XyLang\Library

    hammingDistance (x,y:I32)->(distance:I32) 
    {
        big := x
        ? big < y 
        {
            big = y 
        }
        z := x.XOR.(y)
        count := 0
        
        i := 1
        @ i <= big 
        {
            ? i.AND.(z) ~= 0
            {
                count += 1
            }
            i = i.LFT.(1)
        }    

        <- (count)
    }
}