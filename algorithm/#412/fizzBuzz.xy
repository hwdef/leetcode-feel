Solution
{
    XyLang\Library
}

fizzBuzz (n:i32)->(v:[]str)
{
    arr := []str.{}

    [1<=n].@ 
    {
        ? it % 15 == 0
        {
            arr += "FizzBuzz"
        }
        ? it % 3 == 0 
        {
            arr += "Fizz"
        }
        ? it % 5 == 0
        {
            arr += "Buzz"
        }
        ?
        {
            arr += it.toStr.()
        }
    }
    <- (arr)
}